{
  "description": "Mappings des icônes FontAwesome 4 vers 5",
  "version": "4-to-5",
  "outlined_icons": {
    "description": "Icônes FA4 avec suffixe -o qui deviennent far (regular) en FA5",
    "target_style": "far",
    "mappings": {
      "fa-envelope-o": "fa-envelope",
      "fa-star-o": "fa-star",
      "fa-heart-o": "fa-heart",
      "fa-clock-o": "fa-clock",
      "fa-file-o": "fa-file",
      "fa-folder-o": "fa-folder",
      "fa-folder-open-o": "fa-folder-open",
      "fa-trash-o": "fa-trash",
      "fa-square-o": "fa-square",
      "fa-circle-o": "fa-circle",
      "fa-check-square-o": "fa-check-square",
      "fa-plus-square-o": "fa-plus-square",
      "fa-minus-square-o": "fa-minus-square",
      "fa-times-circle-o": "fa-times-circle",
      "fa-arrow-circle-o-down": "fa-arrow-circle-down",
      "fa-arrow-circle-o-up": "fa-arrow-circle-up",
      "fa-play-circle-o": "fa-play-circle",
      "fa-stop-circle-o": "fa-stop-circle",
      "fa-pause-circle-o": "fa-pause-circle",
      "fa-picture-o": "fa-image",
      "fa-smile-o": "fa-smile",
      "fa-frown-o": "fa-frown",
      "fa-meh-o": "fa-meh",
      "fa-keyboard-o": "fa-keyboard",
      "fa-flag-o": "fa-flag"
    }
  },
  "renamed_icons": {
    "description": "Icônes FA4 renommées en FA5 (sans changement de style)",
    "mappings": {
      "fa-glass": "fa-glass-martini",
      "fa-remove": "fa-times",
      "fa-close": "fa-times",
      "fa-gear": "fa-cog",
      "fa-trash": "fa-trash",
      "fa-home": "fa-home",
      "fa-repeat": "fa-redo",
      "fa-rotate-right": "fa-redo",
      "fa-refresh": "fa-sync",
      "fa-list-alt": "fa-list-alt",
      "fa-dedent": "fa-outdent",
      "fa-video-camera": "fa-video",
      "fa-photo": "fa-image",
      "fa-pencil": "fa-pencil-alt",
      "fa-map-marker": "fa-map-marker-alt",
      "fa-adjust": "fa-adjust",
      "fa-tint": "fa-tint",
      "fa-edit": "fa-edit"
    }
  }
}